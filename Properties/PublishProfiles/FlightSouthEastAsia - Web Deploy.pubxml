<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <SiteUrlToLaunchAfterPublish>http://flightsoutheastasia.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <MSDeployServiceURL>flightsoutheastasia.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>FlightSouthEastAsia</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <UserName>$FlightSouthEastAsia</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="flightConnection" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Server=localahost;Initial Catalog=ukrainflight;TrustServerCertificate=False;" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\flightConnection_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Server=localahost;Initial Catalog=ukrainflight;TrustServerCertificate=False;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
</Project>